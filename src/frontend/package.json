  "homepage": "http://www.cagette.net",
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.9.0",
    "@material-ui/icons": "3.0.1",
    "@material-ui/lab": "^4.0.0-alpha.40",
    "@material-ui/pickers": "^3.2.10",
    "@sentry/browser": "^5.15.5",
    "bootstrap.native": "^2.0.27",
    "bowser": "^2.1.0",
    "cagette-neo": "file:./cagette-neo",
    "date-fns": "^2.9.0",
    "formik": "^2.1.4",
    "formik-material-ui": "^2.0.0-beta.1",
    "formik-material-ui-pickers": "0.0.8",
    "geolib": "^2.0.24",
    "haxe-modular": "^0.7.0",
    "intersection-observer": "^0.5.1",
    "leaflet": "^1.3.1",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "prop-types": "^15.6.2",
    "react": "^16.12.0",
    "react-avatar-editor": "^11.0.7",
    "react-datetime": "^2.9.0",
    "react-dom": "^16.12.0",
    "react-dropzone": "^10.1.10",
    "react-leaflet": "^1.8.0",
    "react-places-autocomplete": "^6.0.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "sticky-events": "^2.3.0",
    "stickyfilljs": "^2.1.0"
  },
  "devDependencies": {
    "browserify": "^14.4.0",
    "cross-env": "^5.1.1",
    "envify": "^4.1.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.2",
    "terser": "^4.4.0",
    "uglifyify": "^5.0.2"
  },
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:js": "haxe cagetteJs.hxml",
    "build:neo": "cp -R ./cagette-neo/public/* ../www",
    "build:sass": "node-sass ../www/css -o ../www/css",
    "watch:sass": "npm run build:sass -- --watch",
    "build2": "babel src -d lib",
    "libs:dev": "npm run build:neo; browserify libs/libs.js -o ../www/js/libs.dev.js -d",
    "libs:prod__old": "npm run build:neo; cross-env NODE_ENV=production browserify libs/libs.js -o ../www/js/libs.js",
    "libs:prod": "npm run build:neo; browserify libs/libs.js -g [ envify --NODE_ENV production ] -g uglifyify | terser --compress --mangle > ../www/js/libs.prod.js",
    "test": "haxe testsJS.hxml && node js/test/tests.js",
    "prepush": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bablukid/cagette.git"
  },
  "bugs": {
    "url": "https://github.com/bablukid/cagette/issues"
  }
}
